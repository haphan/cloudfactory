    "license": "MIT",
    "authors": [
        {
            "name": "Ha Phan",
            "email": "thanhha.work@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "rackspace/php-opencloud": "^1.16",
        "toin0u/digitalocean-v2": "^2.1",
